<ul>
<% @schema[:schema].each do |h| %>
    <ol>
     <% h[:metrics].each do |m| %> 
     <%= h[:fqdn] %> , <%= m[:name] %><pre id="s<%= h[:id] %>_<%= m[:id] %>"></pre><br>

 <script>
    var source = new EventSource('/reports/<%= @schema[:report][:report_id] %>/stat/?image_id=<%= @schema[:report][:report_id] %>&host_id=<%= h[:id] %>&metric_id=<%= m[:id] %>&task_id=<%= m[:task_id]  %>');
    source.addEventListener('stat', function(e) {
      var data = JSON.parse(e.data);
      document.getElementById('s<%= h[:id] %>_<%= m[:id] %>').innerHTML = e.data;
      // alert(data.value)
    }, false);

  </script>

    <% end %>
    </ol>
<% end %>
</ul>
